  "version": "0.2.0",
  "configurations": [
    {
      "name": "TFX - CV List",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}/../main.go",
      "args": [
        "cv",
        "list",
        "--workspaceName=tfx-test",
      ]
    },
    {
      "name": "TFX - Run Show",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}/../main.go",
      "args": [
        "run",
        "show",
        "-i=run-tVxVKhhZeNn8GpZS",
      ]
    },
    {
      "name": "TFX - Plan",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}/../main.go",
      "args": [
        "plan",
        "-w=tfx-test-tfc",
        "-d=./terraform",
        // "--envs=a=b,c=d"
      ]
    },
    {
      "name": "TFX - Apply",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}/../main.go",
      "args": [
        "apply",
        "--runId=run-VCMv2kHVwS4RMVRu",
      ]
    },
    {
      "name": "TFX - PMR List",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}/../main.go",
      "args": [
        "pmr",
        "list",
      ]
    },
    {
      "name": "TFX - Module Download",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}/../main.go",
      "args": [
        "pmr",
        "download",
        "-n=my-module",
        "-p=aws",
        "--moduleVersion=0.0.1"
      ]
    },
    {
      "name": "TFX - PMR Create",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}/../main.go",
      "args": [
        "pmr",
        "create",
        "version",
        "-n=my-module",
        "-p=aws",
        "--moduleVersion=0.0.3",
      ]
    },
    {
      "name": "TFX - TF Versions",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}/../main.go",
      "args": [
        "tfv",
        "list",
        "--enabled=false"
      ]
    },
    {
      "name": "TFX - Test env",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}/../main.go",
      "args": [
        "plan",
        "test",
        "--env", "a=14=33",
      ]
    }
  ]
}